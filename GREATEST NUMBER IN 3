#include <stdio.h>

int main(){
    
    int num1,num2,num3;
    
    printf("enter the value of num1 ");
    scanf("%d",&num1);
    
    printf("enter the value of num2 ");
    scanf("%d",&num2);
    
    printf("enter the value of num3 ");
    scanf("%d",&num3);
    
    //*finding the greatest number in num1 and num2 and num3*//
    {if(num1>num2>num3)
    printf("num1 is maximum ");
    else;
    printf("num2 or num3 is maximum ");}
    
    {if(num2>num3)
    printf("num2 is maximum ");
    else ;
    printf("\n num3 is maximum ");}
    
    return 0;
}
